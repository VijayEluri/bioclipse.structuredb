                <xsd:enumeration value="Mt"/>
                <xsd:enumeration value="N"/>
                <xsd:enumeration value="Na"/>
                <xsd:enumeration value="Nb"/>
                <xsd:enumeration value="Nd"/>
                <xsd:enumeration value="Ne"/>
                <xsd:enumeration value="Ni"/>
