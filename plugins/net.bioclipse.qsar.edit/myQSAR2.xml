<?xml version="1.0" encoding="UTF-8"?>
<qsar:DocumentRoot xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:qsar="http://www.bioclipse.net/qsar">
  <qsar>
    <moleculelist>
      <moleculeResource file="smallCollection.sdf" id="molecule01" name="polycarpol"/>
      <moleculeResource id="molecule02" name="smallCollection"/>
      <moleculeResource id="molecule03" name="reserpine" url="http://pubchem.ncbi.nlm.nih.gov/summary/summary.cgi?cid=5770&amp;disopt=SaveXML"/>
    </moleculelist>
    <descriptorlist>
      <descriptor id="http://www.blueobelisk.org/ontologies/chemoinformatics-algorithms/#xlogP"
          name="XlogP" namespace="http://www.blueobelisk.org">
        <parameter key="checkAromaticity" value="true"/>
        <parameter key="salicylFlag" value="false"/>
        <descriptorimpl/>
      </descriptor>
      <descriptor id="http://www.blueobelisk.org/ontologies/chemoinformatics-algorithms/#atomCount"
          name="Atom Count" namespace="http://www.blueobelisk.org">
        <parameter key="elementName" value="C"/>
        <descriptorimpl id="org.openscience.cdk.qsar.descriptors.molecular.XLogPDescriptor"
            namespace="http://rguha.ath.cx/~rguha/cicc/desc/descriptors/"/>
      </descriptor>
      <descriptor id="http://www.blueobelisk.org/ontologies/chemoinformatics-algorithms/#atomCount"
          name="DragonDesc1" namespace="http://www.blueobelisk.org">
        <descriptorimpl id="dragon"/>
      </descriptor>
    </descriptorlist>
    <descriptorimpl id="cdk" name="Chemistry Development Kit" namespace="http://cdk.sourceforge.net"
        vendor="Chemistry Development Kit" version="1.1.0.20080808"/>
    <descriptorimpl id="dragon" name="Dragon descriptor" namespace="http://dragon.com"
        vendor="MOLMOLMOL" version="5.14"/>
    <preprocessing>
      <preprocessingStep id="Smi23d" name="Generate 3D coordinates with smi23d" namespace="http://www.chembiogrid.org/cheminfo/smi23d/"
          order="1"/>
      <preprocessingStep id="org.openscience.cdk.atomtype.sybyl" name="Sybyl Atom Types"
          namespace="http://cdk.sf.net" order="2"/>
    </preprocessing>
  </qsar>
</qsar:DocumentRoot>
