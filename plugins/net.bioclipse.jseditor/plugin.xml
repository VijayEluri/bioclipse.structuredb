<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="net.bioclipse.jseditor.editors.JsEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            default="false"
            extensions="js"
            icon="icons/png/jsfile.png"
            id="net.bioclipse.jseditor.editor"
            name="Javascript Editor">
      </editor>
   </extension>

  <extension point="org.eclipse.ui.newWizards">
      <wizard
		    category="net.bioclipse.ui.scripting"
            canFinishEarly="true"
            icon="icons/png/jsfile.png"
            class="net.bioclipse.jseditor.wizards.NewJavaScriptWizard"
            hasPages="false"
            id="net.bioclipse.jseditor.wizards.NewJavaScriptWizard"
            name="JavaScript"/>
   </extension>

</plugin>
